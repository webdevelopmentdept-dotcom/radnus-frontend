{
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)(bot|crawler|spider|bing|google|yahoo|duckduck|baidu|facebook|twitter|linkedin|embedly|slack|telegram|whatsapp|pinterest)"
        }
      ],
      "destination": "https://radnus-backend.onrender.com/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
